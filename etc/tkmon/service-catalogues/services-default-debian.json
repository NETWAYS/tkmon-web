    "data": [
        {
            "service_description": "net-ping",
            "display_name": "Ping check",
            "check_command": {
                "command_name": "check_ping",
                "arguments": [
                    {
                        "label": "Warning threshold",
                        "description": {
                            "en_US": "In form <0000,000%>, <miliseconds rtt>,<percent rta><%>",
                            "de_DE": "In der form <0000,000%>, <miliseconds rtt>,<percent rta><%>"
                        },
                        "validation": "string",
                        "argument": "-w",
                        "value": "5000,100%"
                    },
                    {
                        "label": "Warning threshold",
                        "description": {
                            "en_US": "In form <0000,000%>, <miliseconds rtt>,<percent rta><%>",
                            "de_DE": "In der Form <0000,000%>, <miliseconds rtt>,<percent rta><%>"
                        },
                        "validation": "string",
                        "argument": "-c",
                        "value": "5000,100%"
                    }
                ]
            },
            "_catalogue_attributes": {
                "tags": ["remote", "ping", "network"],
                "name": "net-ping",
                "label": {
                    "en_US": "Ping check",
                    "de_DE": "Ping check"
                },
                "description": {
                    "en_US": "Check packet loss and round trip time",
                    "de_DE": "Prüfen auf Paketverlust und Laufzeiten"
                },
                "defined": true,
                "defined_in": "/etc/nagios-plugins/config/ping.cfg"
            }
        },
