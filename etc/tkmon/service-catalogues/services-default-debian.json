{
    "type": "service",
    "version": "1.0",
    "description": "Template for checks defined in nagios-plugins packages",
    "data": [
        {
            "service_description": "net-ping",
            "display_name": "Ping check",
            "check_command": {
                "command_name": "check_ping",
                "arguments": [
                    {
                        "label": "Warning threshold",
                        "description": "In form <0000,000%>, <miliseconds rtt>,<percent rta><%>",
                        "validation": "string",
                        "argument": "-w",
                        "value": "5000,100%"
                    },
                    {
                        "label": "Warning threshold",
                        "description": "In form <0000,000%>, <miliseconds rtt>,<percent rta><%>",
                        "validation": "string",
                        "argument": "-c",
                        "value": "5000,100%"
                    }
                ]
            },
            "_catalogue_attributes": {
                "tags": ["remote", "ping", "network"],
                "name": "net-ping",
                "label": "Ping check",
                "description": "Check packet loss and round trip time",
                "defined": true,
                "defined_in": "/etc/nagios-plugins/config/ping.cfg"
            }
        },
        {
            "service_description": "net-tcp",
            "display_name": "TCP Port",
